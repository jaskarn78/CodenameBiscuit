<?xml version="1.0" encoding="utf-8"?>

<com.example.codenamebiscuit.swipedeck.layouts.SwipeFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/swipeLayout"
    xmlns:swipedeck="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/swipeBackground"
        android:scaleType="fitXY"
      />
    <com.example.codenamebiscuit.swipedeck.SwipeDeck
        android:id="@+id/swipe_deck"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="80dp"
        android:paddingStart="3dp"
        android:paddingEnd="3dp"
        swipedeck:max_visible="2"
        swipedeck:card_spacing="15dp"
        swipedeck:render_above="true"
        swipedeck:rotation_degrees="15"
        swipedeck:opacity_end="0.8"
        />




</com.example.codenamebiscuit.swipedeck.layouts.SwipeFrameLayout>



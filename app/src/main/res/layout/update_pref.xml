

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">



    <net.cachapa.expandablelayout.ExpandableLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:fancy="http://schemas.android.com/apk/res-auto"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/expandable_layout"
        android:background="@color/transparentFB"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:el_duration="800"
        app:el_expanded="false">


            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:fancy="http://schemas.android.com/apk/res-auto"
                android:id="@+id/section_socialnetworks"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:gravity="center">

                    <mehdi.sakout.fancybuttons.FancyButton
                        android:id="@+id/btn_music"
                        android:layout_width="105dp"
                        android:layout_height="105dp"
                        fancy:fb_defaultColor="@color/translivinPink"
                        fancy:fb_focusColor="#313131"
                        fancy:fb_fontIconSize="10dp"
                        fancy:fb_iconPaddingLeft="00dp"
                        fancy:fb_iconPaddingRight="00dp"
                        fancy:fb_iconPosition="top"
                        fancy:fb_iconResource="@drawable/musicnote"
                        fancy:fb_radius="64dp"
                        fancy:fb_text="Music"
                        fancy:fb_textColor="@color/livinBlack" />

                    <mehdi.sakout.fancybuttons.FancyButton
                        android:id="@+id/btn_food"
                        android:layout_width="105dp"
                        android:layout_height="105dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginStart="10dp"
                        fancy:fb_defaultColor="@color/translivinPink"
                        fancy:fb_focusColor="#313131"
                        fancy:fb_iconPaddingLeft="0dp"
                        fancy:fb_iconPaddingRight="0dp"
                        fancy:fb_iconPosition="top"
                        fancy:fb_iconResource="@drawable/hamburger"
                        fancy:fb_radius="64dp"
                        fancy:fb_text="Food"
                        fancy:fb_textColor="@color/livinBlack">

                    </mehdi.sakout.fancybuttons.FancyButton>


                    <mehdi.sakout.fancybuttons.FancyButton
                        android:id="@+id/btn_sports"
                        android:layout_width="105dp"
                        android:layout_height="105dp"
                        fancy:fb_defaultColor="@color/translivinPink"
                        fancy:fb_focusColor="#313131"
                        fancy:fb_iconPaddingLeft="0dp"
                        fancy:fb_iconPaddingRight="0dp"
                        fancy:fb_iconPosition="top"
                        fancy:fb_iconResource="@drawable/football"
                        fancy:fb_radius="64dp"
                        fancy:fb_text="Sports"
                        fancy:fb_textColor="@color/livinBlack">

                    </mehdi.sakout.fancybuttons.FancyButton>


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/sn_pos2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <mehdi.sakout.fancybuttons.FancyButton
                        android:id="@+id/btn_outdoors"
                        android:layout_width="105dp"
                        android:layout_height="105dp"
                        fancy:fb_defaultColor="@color/translivinPink"
                        fancy:fb_focusColor="#313131"
                        fancy:fb_iconPaddingLeft="0dp"
                        fancy:fb_iconPaddingRight="0dp"
                        fancy:fb_iconPosition="top"
                        fancy:fb_iconResource="@drawable/basecamp"
                        fancy:fb_radius="64dp"
                        fancy:fb_text="Outdoor"
                        fancy:fb_textColor="@color/livinBlack">

                    </mehdi.sakout.fancybuttons.FancyButton>

                    <mehdi.sakout.fancybuttons.FancyButton
                        android:id="@+id/btn_health"
                        android:layout_width="105dp"
                        android:layout_height="105dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginStart="10dp"
                        fancy:fb_defaultColor="@color/translivinPink"
                        fancy:fb_focusColor="#313131"
                        fancy:fb_iconPaddingLeft="0dp"
                        fancy:fb_iconPaddingRight="0dp"
                        fancy:fb_iconPosition="top"
                        fancy:fb_iconResource="@drawable/bike"
                        fancy:fb_radius="64dp"
                        fancy:fb_text="Health"
                        fancy:fb_textColor="@color/livinBlack">

                    </mehdi.sakout.fancybuttons.FancyButton>

                    <mehdi.sakout.fancybuttons.FancyButton
                        android:id="@+id/btn_family"
                        android:layout_width="105dp"
                        android:layout_height="105dp"
                        fancy:fb_defaultColor="@color/translivinPink"
                        fancy:fb_focusColor="#313131"
                        fancy:fb_iconPaddingLeft="0dp"
                        fancy:fb_iconPaddingRight="0dp"
                        fancy:fb_iconPosition="top"
                        fancy:fb_iconResource="@drawable/ic_face_black_24dp"
                        fancy:fb_radius="64dp"
                        fancy:fb_text="Family"
                        fancy:fb_textColor="@color/livinBlack">

                    </mehdi.sakout.fancybuttons.FancyButton>


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/LinearLayout1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <mehdi.sakout.fancybuttons.FancyButton
                        android:id="@+id/btn_retail"
                        android:layout_width="105dp"
                        android:layout_height="105dp"
                        fancy:fb_defaultColor="@color/translivinPink"
                        fancy:fb_focusColor="#313131"
                        fancy:fb_iconPaddingLeft="0dp"
                        fancy:fb_iconPaddingRight="0dp"
                        fancy:fb_iconPosition="top"
                        fancy:fb_iconResource="@drawable/cart"
                        fancy:fb_radius="64dp"
                        fancy:fb_text="Retail"
                        fancy:fb_textColor="@color/livinBlack">

                    </mehdi.sakout.fancybuttons.FancyButton>

                    <mehdi.sakout.fancybuttons.FancyButton
                        android:id="@+id/btn_performing"
                        android:layout_width="105dp"
                        android:layout_height="105dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginStart="10dp"
                        fancy:fb_defaultColor="@color/translivinPink"
                        fancy:fb_focusColor="#313131"
                        fancy:fb_iconPaddingLeft="0dp"
                        fancy:fb_iconPaddingRight="0dp"
                        fancy:fb_iconPosition="top"
                        fancy:fb_iconResource="@drawable/theater"
                        fancy:fb_radius="64dp"
                        fancy:fb_text="Entertainment"
                        fancy:fb_textColor="@color/livinBlack"
                        fancy:fb_textSize="11sp">

                    </mehdi.sakout.fancybuttons.FancyButton>

                    <mehdi.sakout.fancybuttons.FancyButton
                        android:id="@+id/btn_entertainment"
                        android:layout_width="105dp"
                        android:layout_height="105dp"
                        fancy:fb_defaultColor="@color/translivinPink"
                        fancy:fb_focusColor="#313131"
                        fancy:fb_iconPaddingLeft="0dp"
                        fancy:fb_iconPaddingRight="0dp"
                        fancy:fb_iconPosition="top"
                        fancy:fb_iconResource="@drawable/movie"
                        fancy:fb_radius="64dp"
                        fancy:fb_text="Charity"
                        fancy:fb_textColor="@color/livinBlack"
                        fancy:fb_textSize="12sp">
                    </mehdi.sakout.fancybuttons.FancyButton>
                </LinearLayout>
                <com.mikepenz.iconics.view.IconicsImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_gravity="right"
                    android:layout_marginBottom="10dp"
                    android:id="@+id/closeLayout"
                    android:src="@drawable/ic_close_white_36dp"
                    android:tint="@color/livinPink"
                    android:layout_marginRight="15dp"
                    android:clickable="true"
                    />

            </LinearLayout>
    </net.cachapa.expandablelayout.ExpandableLayout>
</LinearLayout>